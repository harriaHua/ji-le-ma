.dialog {
  width: 80%;
  height: 150px;
  background: #fa4646;
  border-radius: 10px;
  position: absolute;
  bottom: 15%;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.dialog ._p {
  font-size: 24px;
  color: #fff;
  margin-bottom: 30px;
  display: flex;
  justify-content: center;
  margin-top: 40px;
}
.dialog button {
  background: #a8abb0;
